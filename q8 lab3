#include <iostream>
using namespace std;

int main() {
	char a;
	cout<<"Enter an alphabet : \n";
	cin>>a;
	if(a == 'a' || a == 'e' || a == 'i' || a == 'o' || a == 'u' || a =='A' || a=='E' || a== 'I' || a== 'O' || a == 'U')
	{
		cout<<"The character is a vowel";
	}
	else
	{
		cout<<"The character is a consonant";
	}
	return 0;
}
