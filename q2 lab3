#include <iostream>
using namespace std;

int main() {
	float a, b, c;
	cout << "Enter three numbers\n";
	cin >> a >> b >> c;
	if (a>b && a>c)
	{
		cout << "The largest number =" << a;
	}
	if (b>c && b>a)
	{
		cout << "The largest number = " << b;
	}
	if(c>a && c>b)
	{
		cout << "The largest number is " << c;
	}
	return 0;
}
