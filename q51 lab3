#include <iostream>
using namespace std;

int main() {
	int n, x, rem, sum = 0;
	cout << "Enter a number\n";
	cin >> n;
	x=n;
	while(x!=0)
	{
		rem = x%10;
		sum = sum + rem*rem*rem;
		x=x/10;
	}
	if(sum = n)
	{
		cout << "The number is an armstrong number";
	}
	else
	{
		cout << "The number is not an armstrong number";
	}
	return 0;
}
