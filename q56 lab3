#include <iostream>
using namespace std;

int main() {
	int n, rem, n1, i, num, number, j, x;
	cout << "Enter any number\n";
	cin >> number;
	for(j=1; j<number; j++)
	{ 
		x=j;
	while(x!=0)
	{
	rem = x%10;
	n1=1;
	num=0;
	for(i=1; i<=rem; i++)
	{
		n1=n1*i;
	}
	x=x/10;
	num = num + n1;
	}
	if(num == j)
	{
		cout << j << " ";
	}
	}
	return 0;
}
