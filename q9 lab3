#include <iostream>
using namespace std;

int main() {
	char a;
	cout<<"Enter an character : \n";
	cin>>a;
	if(((a >= 'A') && (a<='Z')) || ((a >='a') && (a <= 'z')))
	{
		cout << "You entered an Alphabet";
	}
	else if(a >= '0' && a <='9')
	{
		cout << "You entered a digit";
	}
	else
	{
		cout << "You entered a special character";
	}
	return 0;
}
