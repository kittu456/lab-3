#include <iostream>
using namespace std;

int main() {
	int n, i;
	bool prime= true;
	cout << "Enter a number";
	cin >> n;
	for(i=2; i<n; i++)
	{
		if(n%i==0)
		{
			prime=false;
			break;
		}
	}
	if(prime)
	{
		cout << "This is a prime number";
	}
	else
	{
		cout << "This is not a prime number";
	}
	return 0;
}
